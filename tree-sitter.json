{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "rockstar",
      "camelcase": "Rockstar",
      "title": "Rockstar",
      "scope": "source.rockstar",
      "file-types": [
        "rock"
      ],
      "injection-regex": "^rockstar$",
      "class-name": "TreeSitterRockstar"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Rockstar grammar for tree-sitter",
    "authors": [
      {
        "name": "Moritz Tomann"
      }
    ],
    "links": {
      "repository": "https://github.com/TomannMoritz/tree-sitter-rockstar"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}